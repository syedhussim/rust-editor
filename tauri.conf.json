{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Rust IDE",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "./ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "decorations": true,
        "title": "Rust IDE",
        "fullscreen": false,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
    
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
